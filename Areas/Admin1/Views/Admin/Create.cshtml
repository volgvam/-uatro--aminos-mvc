@model CuatroCaminosMvcApplication.Models.Administrators

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Administrators</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Код_Ученика)
        </div>
        <div class="editor-field">
            @Html.DropDownList("Код_Ученика", "Выберите имя")
            @Html.ValidationMessageFor(model => model.Код_Ученика)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Название_танца)
        </div>
        <div class="editor-field">
            @Html.DropDownList("Название_танца", "Выберите стиль танца")
            @Html.ValidationMessageFor(model => model.Название_танца)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
